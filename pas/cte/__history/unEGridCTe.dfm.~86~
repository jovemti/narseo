  inherited qXML: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO cte_xml'
      '  (cte, xml_cte, xml_cancelamento)'
      'VALUES'
      '  (:cte, :xml_cte, :xml_cancelamento)')
    SQLDelete.Strings = (
      'DELETE FROM cte_xml'
      'WHERE'
      '  codigo = :Old_codigo')
    SQLUpdate.Strings = (
      'UPDATE cte_xml'
      'SET'
      
        '  cte = :cte, xml_cte = :xml_cte, xml_cancelamento = :xml_cancel' +
        'amento'
      'WHERE'
      '  codigo = :Old_codigo')
    SQLLock.Strings = (
      'SELECT * FROM cte_xml'
      'WHERE'
      '  codigo = :Old_codigo'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      'SELECT cte, xml_cte, xml_cancelamento FROM cte_xml'
      'WHERE'
      '  codigo = :codigo')
    SQLRecCount.Strings = (
      'SELECT count(*) FROM ('
      'SELECT * FROM cte_xml'
      ''
      ') t')
    SQL.Strings = (
      'SELECT codigo, cte, xml_cte, xml_cancelamento'
      '  FROM cte_xml'
      ' WHERE cte = :codigo;')
    MasterSource = ds
    MasterFields = 'codigo'
    DetailFields = 'cte'
    ParamData = <
      item
        DataType = ftInteger
        Name = 'codigo'
        ParamType = ptInput
        Value = nil
      end>
    object FieldXMLcodigo: TIntegerField
      FieldName = 'codigo'
    end
    object qXMLcte: TLargeintField
      FieldName = 'cte'
      Required = True
    end
    object qXMLxml_cte: TMemoField
      FieldName = 'xml_cte'
      BlobType = ftMemo
    end
    object qXMLxml_cancelamento: TMemoField
      FieldName = 'xml_cancelamento'
      BlobType = ftMemo
    end
  end
