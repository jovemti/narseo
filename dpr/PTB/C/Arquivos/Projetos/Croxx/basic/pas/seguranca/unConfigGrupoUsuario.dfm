  object qFomularios: TUniQuery
    Connection = Connection.Conn
    SQL.Strings = (
      'SELECT sf.codigo, '
      #9'sf.nome, '
      #9'sf.formname, '
      #9'sf.tag, '
      #9'sf.modulo, '
      #9'sf.ativo'
      '   FROM system_formularios sf'
      
        '  WHERE sf.modulo = :modulo and sf.codigo <> 1 and sf.codigo not' +
        ' in (SELECT formulario '
      #9#9#9'    FROM system_permissoes_grupo spg '
      #9#9#9'   WHERE grupo= :grupo)'
      '  ORDER BY sf.tag ;')
    Left = 456
    Top = 96
    ParamData = <
